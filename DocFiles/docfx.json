{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "Sztorm.Collections/**.cs"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "**.log.cs",
            "**.tt"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "globalMetadata": {
      "_disableContribution": true
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "src": "../",
        "files": [
          "DocFiles/apidoc/**.md"
        ],
        "exclude": [
          "DocFiles/obj/**",
          "docs/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false
  }
}